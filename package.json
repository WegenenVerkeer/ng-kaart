{
  "name": "ng-kaart",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve testApp --port 4220 --disable-host-check --host 0.0.0.0 --base-href '/ng-kaart/'",
    "start-apigateway": "ng serve testApp --port 4220 --public-host https://apigateway/ --deploy-url https://apigateway/ng-kaart/ --base-href /ng-kaart --disable-host-check --host 0.0.0.0 --serve-path '/ng-kaart'",
    "start-wc-apigateway": "ng serve webcomponent --port 4220 --public-host http://apigateway/ --deploy-url http://apigateway/ng-kaart/ --base-href /ng-kaart --disable-host-check --host 0.0.0.0 --serve-path '/ng-kaart'",
    "watch": "ng build --watch",
    "clean": "rimraf out; rimraf dist; mkdirp out",
    "build": "npm ci && npm prune && ng build && cp -R src/workbox dist/ng-kaart",
    "build-prod": "npm ci && npm prune && ng build --prod && cp -R src/workbox dist/ng-kaart",
    "pretest": "npm ci && npm prune",
    "test": "ng test ng-kaart --watch=false --code-coverage --browsers=ChromeHeadlessNoSandbox",
    "test-debug": "karma start karma.conf.js --watch=true --browsers=PhantomJS --debug",
    "build-test-app": "npm ci && npm prune && npm run clean && ng build && ng build testApp --base-href /ng-kaart --deploy-url /ng-kaart/",
    "lint": "eslint -c .eslintrc.js \"src/**/*.ts\"",
    "lint-fix": "eslint -c .eslintrc.js --fix \"src/**/*.ts\" --fix \"projects/ng-kaart/src/**/*.ts\"",
    "e2e": "ng e2e",
    "cypress": "cypress open",
    "start:e2e": "ng serve testApp --port 4221",
    "cy:run": "CYPRESS_baseUrl=http://localhost:4221/ cypress run",
    "cy:e2e": "start-server-and-test start:e2e http-get://localhost:4221/ cy:run",
    "publishToNexus": "node script/patchRegistry.js https://collab.mow.vlaanderen.be/artifacts/repository/npm-internal/",
    "publishToNpmjs": "node script/patchRegistry.js",
    "build:elements": "ng build --prod --output-hashing none webcomponent && npm run generate-docs && node script/elements-build.js",
    "compodoc": "compodoc",
    "generate-docs": "compodoc projects/ng-kaart/src/lib/classic -c projects/ng-kaart/.compodocrc.json -d ./dist/webcomponent/documentation -p projects/ng-kaart/tsconfig.lib.json",
    "serve-docs": "compodoc -s projects/ng-kaart/tsconfig.lib.json"
  },
  "lint-staged": {
    "linters": {
      "src/**/*.ts": [
        "eslint -c .eslintrc.js --fix \"src/**/*.ts\"",
        "awk /apps-(dev|tei).mow.vlaanderen.be/{exit(1)}",
        "git add"
      ],
      "projects/ng-kaart/**/*.ts": [
        "eslint -c .eslintrc.js --fix \"projects/ng-kaart/src/**/*.ts\"",
        "awk /apps-(dev|tei).mow.vlaanderen.be/{exit(1)}",
        "git add"
      ]
    },
    "ignore": [
      "**/test.ts"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.4",
    "@angular/cdk": "^10.2.3",
    "@angular/common": "^10.2.4",
    "@angular/compiler": "^10.2.4",
    "@angular/core": "^10.2.4",
    "@angular/elements": "^10.2.4",
    "@angular/forms": "^10.2.4",
    "@angular/material": "^10.2.3",
    "@angular/material-moment-adapter": "^10.2.3",
    "@angular/platform-browser": "^10.2.4",
    "@angular/platform-browser-dynamic": "^10.2.4",
    "@angular/router": "^10.2.4",
    "@angular/service-worker": "^10.2.4",
    "@turf/boolean-intersects": "6.0.2",
    "@turf/turf": "5.1.6",
    "@types/uuid": "8.3.0",
    "core-js": "2.6.9",
    "document-register-element": "^1.14.6",
    "drag-drop-touch": "^1.3.1",
    "fp-ts": "^2.10.2",
    "idb": "3.0.2",
    "loglevel": "1.6.1",
    "luxon": "^1.25.0",
    "material-design-icons": "3.0.1",
    "moment": "^2.27.0",
    "monocle-ts": "^2.3.9",
    "motion-sensors-polyfill": "^0.3.1",
    "mustache": "3.2.1",
    "newtype-ts": "^0.3.4",
    "ng4-click-outside": "1.0.1",
    "ol": "^6.4.2",
    "proj4": "2.6.2",
    "resize-observer": "1.0.0",
    "rxjs": "^6.6.3",
    "scroll-into-view-if-needed": "^2.2.26",
    "tslib": "2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.7",
    "@angular-devkit/build-ng-packagr": "^0.1001.7",
    "@angular-eslint/eslint-plugin": "^0.2.0-beta.1",
    "@angular/cli": "10.1.0",
    "@angular/compiler-cli": "^10.2.4",
    "@angular/language-service": "^10.2.4",
    "@compodoc/compodoc": "^1.1.9",
    "@types/googlemaps": "3.30.16",
    "@types/hammerjs": "2.0.36",
    "@types/jasmine": "^2.8.17",
    "@types/jasmine-jquery": "1.5.33",
    "@types/jasminewd2": "~2.0.3",
    "@types/loglevel": "1.5.3",
    "@types/luxon": "^1.24.4",
    "@types/mocha": "^5.2.5",
    "@types/mustache": "0.8.32",
    "@types/node": "^12.12.54",
    "@types/ol": "^6.4.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "concat": "^1.0.3",
    "copy-webpack-plugin": "4.6.0",
    "cypress": "^12.10.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fs-extra": "^7.0.1",
    "glob": "7.1.3",
    "husky": "^1.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-jquery": "2.1.1",
    "jasmine-reporters": "2.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-intl-shim": "1.0.3",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "1.2.0",
    "karma-mocha-reporter": "2.2.4",
    "karma-phantomjs-launcher": "1.0.4",
    "lint-staged": "8.2.1",
    "mkdirp": "^0.5.5",
    "ng-packagr": "^10.1.0",
    "prettier": "^2.1.1",
    "prettier-eslint": "^11.0.0",
    "protractor": "~7.0.0",
    "protractor-console": "3.0.0",
    "protractor-console-plugin": "0.1.1",
    "protractor-http-mock": "0.9.2",
    "protractor-jasmine2-screenshot-reporter": "0.4.0",
    "request": "^2.88.2",
    "request-promise-native": "1.0.5",
    "rimraf": "^2.7.1",
    "start-server-and-test": "^1.11.3",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.7",
    "typescript-formatter": "^7.2.2",
    "webdriver-manager": "^12.1.7",
    "webpack-dev-server": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
