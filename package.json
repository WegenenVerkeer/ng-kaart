{
  "name": "@wegenenverkeer/ng-kaart",
  "version": "2.1.3",
  "description": "AWV Angular 5 kaart component.",
  "keywords": [
    "AWV",
    "Kaart",
    "Angular",
    "Google locator"
  ],
  "author": "AWV",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:WegenenVerkeer/ng-kaart.git"
  },
  "scripts": {
    "preinstall": "npm config set package-lock false",
    "ng": "ng",
    "prestart": "npm install; npm prune",
    "start": "ng serve --port 4220 --host 0.0.0.0",
    "preclean": "npm install rimraf mkdirp",
    "clean": "rimraf out; rimraf dist; mkdirp out",
    "preinline-resources": "rsync -a src/lib/ out/inline-resources",
    "inline-resources": "node inline-resources.js out/inline-resources",
    "compile-es2015": "ngc -p out/inline-resources/tsconfig.json",
    "compile-es5": "ngc -p out/inline-resources/tsconfig.es5.json",
    "rollup-es2015": "rollup out/tsc/lib-es2015/ng-kaart.js -o dist/ng-kaart.js -f es -c",
    "postrollup-es2015": "replace 'openlayers/index' 'openlayers' dist/ng-kaart.js",
    "rollup-es5": "rollup out/tsc/lib-es5/ng-kaart.js -o dist/ng-kaart.es5.js -f es -c",
    "postrollup-es5": "replace 'openlayers/index' 'openlayers' dist/ng-kaart.es5.js",
    "prebuild": "npm run clean; npm install; npm prune; npm run inline-resources; npm run lint",
    "build-es2015": "npm run compile-es2015; npm run rollup-es2015",
    "build-es5": "npm run compile-es5; npm run rollup-es5",
    "build": "npm run build-es2015; npm run build-es5",
    "postbuild": "rsync -a --exclude=*.js --exclude=*.js.map out/tsc/lib-es2015/ dist/; cp src/lib/package.json dist/package.json; cp README.md dist/README.md",
    "devel": "onchange 'src/**/*.ts' -i -- npm run build",
    "lint": "tslint --project tsconfig.json \"src/**/*.ts\" --exclude \"src/**/*.d.ts\"",
    "lint-fix": "npm run lint -- --fix",
    "pretest": "npm install; npm prune",
    "test": "ng test --single-run --code-coverage",
    "test-debug": "karma start karma.conf.js --single-run=false --browsers=PhantomJS --debug",
    "e2e": "env LANG=en_US.UTF-8 ng e2e",
    "publishToNexus": "node script/patchRegistry.js https://collab.mow.vlaanderen.be/artifacts/repository/npm-internal/",
    "publishToNpmjs": "node script/patchRegistry.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "linters": {
      "src/**/*.ts": [
        "tslint --type-check --project tsconfig.json --fix",
        "git add"
      ]
    }
  },
  "dependencies": {
    "@angular/animations": "5.1.3",
    "@angular/common": "5.1.3",
    "@angular/compiler": "5.1.3",
    "@angular/core": "5.1.3",
    "@angular/forms": "5.1.3",
    "@angular/http": "5.1.3",
    "@angular/platform-browser": "5.1.3",
    "@angular/router": "5.1.3",
    "@types/lodash": "4.14.91",
    "copy-webpack-plugin": "4.4.1",
    "core-js": "2.5.3",
    "fp-ts": "0.6.8",
    "googlemaps": "1.12.0",
    "hammerjs": "2.0.8",
    "immutable": "3.8.2",
    "lodash-es": "4.17.4",
    "loglevel": "1.6.1",
    "material-design-icons": "3.0.1",
    "ng4-click-outside": "1.0.1",
    "openlayers": "4.6.4",
    "proj4": "2.4.4",
    "rxjs": "5.5.5",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.6.5",
    "@angular/compiler-cli": "5.1.3",
    "@angular/language-service": "5.1.3",
    "@angular/platform-browser-dynamic": "5.1.3",
    "@types/chance": "0.7.35",
    "@types/googlemaps": "3.30.2",
    "@types/hammerjs": "2.0.35",
    "@types/jasmine": "2.5.54",
    "@types/jasmine-jquery": "1.5.32",
    "@types/node": "8.5.1",
    "@types/openlayers": "4.3.11",
    "chance": "1.0.11",
    "codelyzer": "4.0.2",
    "glob": "7.1.1",
    "husky": "0.14.3",
    "jasmine-core": "2.8.0",
    "jasmine-jquery": "2.1.1",
    "jasmine-reporters": "2.2.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "1.7.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-intl-shim": "1.0.3",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-jquery": "0.1.1",
    "karma-junit-reporter": "1.2.0",
    "karma-mocha-reporter": "2.2.4",
    "karma-phantomjs-launcher": "1.0.4",
    "lint-staged": "6.0.0",
    "mkdirp": "^0.5.1",
    "phantomjs": "2.1.1",
    "prettier": "1.9.2",
    "protractor": "5.1.2",
    "protractor-console": "3.0.0",
    "protractor-console-plugin": "0.1.1",
    "protractor-http-mock": "0.9.2",
    "protractor-jasmine2-screenshot-reporter": "0.4.0",
    "replace": "0.3.0",
    "request": "2.81.0",
    "request-promise-native": "1.0.4",
    "rimraf": "^2.6.2",
    "rollup": "0.52.3",
    "rollup-plugin-sourcemaps": "0.4.2",
    "ts-node": "4.0.2",
    "tslint": "5.8.0",
    "tslint-config-prettier": "1.6.0",
    "tslint-defocus": "2.0.3",
    "tslint-plugin-prettier": "1.3.0",
    "typescript": "2.5.2"
  },
  "publishConfig": {
    "registry": "https://collab.mow.vlaanderen.be/artifacts/repository/npm-internal/"
  }
}
